alias ..="cd .."
alias ...="cd ../.."

alias l='ls'
alias la='ls -a'
alias ll='ls -l'
alias lla='ls -la'

alias o=open
alias t="open -a Terminal"

alias m='mvim'

# Git
alias gs='git status'
alias ga='git add'
alias gd='git diff'
alias gc='git commit -v'
alias gps='git push'
alias gpl='git pull'
alias glog='git log'

alias r='rails'
